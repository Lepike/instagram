@model InstagramAccount

@{
    ViewBag.Title = "GetToken";
    Layout = "/Views/Shared/master.cshtml";
}

<div class="container">
    <h2>GetToken</h2>
    @Model.user.full_name <br />
    <img src="@Model.user.profile_picture" alt="" /> <br />
    userid = @Model.user.id<br />
    access_token = @Model.access_token 
</div>

